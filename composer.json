{
    "name": "sdis62/prevarisc-passerelle-platau",
    "type": "application",
    "description": "Une passerelle d'import / export automatis√©e entre Prevarisc et Plat'AU",
    "repositories": [
        {
            "type": "path",
            "url": "./lib/syncplicity-sdk"
        },
        {
            "type": "path",
            "url": "./lib/platau-sdk"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "league/flysystem": "^2.0",
        "uma/dic": "^1.0",
        "adbario/php-dot-notation": "^3.2",
        "symfony/console": "^5.2",
        "doctrine/dbal": "^3.0",
        "symfony/options-resolver": "^5.2",
        "sdis62/platau-sdk": "*"
    },
    "require-dev": {
        "kdubuc/php-cs-fixer-rules": "^1.0",
        "vimeo/psalm": "^5.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "bin": [
        "bin/platau"
    ],
    "minimum-stability": "dev"
}
